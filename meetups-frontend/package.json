{
  "name": "frontend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"yarn run dev\" \"yarn run dts --watch\" \"yarn run graphql-types --watch\"",
    "dev": "node scripts/start.js",
    "dts": "tcm src -c -o src_gen",
    "graphql-types": "apollo client:codegen --target typescript --tagName graphql --globalTypesFile=src/types/global.ts types"
  },
  "devDependencies": {
    "express": "^4.16.4",
    "graphql-tag": "^2.10.0",
    "http-proxy-middleware": "^0.19.1",
    "parcel-bundler": "^1.10.3",
    "postcss-modules": "^1.4.1",
    "prettier": "1.15.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typed-css-modules": "^0.3.7",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@hackclub/design-system": "^0.0.1-14",
    "@reach/router": "^1.2.1",
    "@types/color": "^3.0.0",
    "@types/randomcolor": "^0.5.0",
    "@types/reach__router": "^1.2.2",
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "@types/webpack-env": "^1.13.6",
    "apollo": "^2.1.8",
    "apollo-cache-inmemory": "^1.3.11",
    "apollo-client": "^2.4.7",
    "apollo-link": "^1.2.4",
    "apollo-link-error": "^1.1.2",
    "apollo-link-http": "^1.5.7",
    "color": "^3.1.0",
    "color-scheme": "^1.0.1",
    "components": "^0.1.0",
    "concurrently": "^4.1.0",
    "graphql": "^14.0.2",
    "hello-color": "^1.0.1",
    "palx": "^1.0.3",
    "randomcolor": "^0.5.3",
    "react": "^16.6.3",
    "react-apollo": "^2.3.2",
    "react-dom": "^16.6.3",
    "styled-components": "^3.4.9",
    "tinycolor2": "^1.4.1"
  },
  "postcss": {
    "modules": true,
    "plugins": {
      "autoprefixer": {
        "browsers": [
          ">1%",
          "last 2 versions",
          "not ie < 11"
        ]
      },
      "postcss-modules": {
        "camelCase": true,
        "generateScopedName": "[name]_[local]_[hash:base64:5]"
      }
    }
  }
}
