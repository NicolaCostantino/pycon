{
  "name": "frontend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"yarn run dev\" \"yarn run dts --watch\"",
    "dev": "parcel src/index.html",
    "dts": "tcm src -c -o src_gen"
  },
  "devDependencies": {
    "parcel-bundler": "^1.9.7",
    "postcss-modules": "^1.3.2",
    "typed-css-modules": "^0.3.5",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@reach/router": "^1.1.1",
    "@types/reach__router": "^1.0.1",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "@types/webpack-env": "^1.13.6",
    "components": "^0.1.0",
    "concurrently": "^4.0.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "postcss": {
    "modules": true,
    "plugins": {
      "autoprefixer": {
        "browsers": [
          ">1%",
          "last 2 versions",
          "not ie < 11"
        ]
      },
      "postcss-modules": {
        "camelCase": true,
        "generateScopedName": "[name]_[local]_[hash:base64:5]"
      }
    }
  }
}